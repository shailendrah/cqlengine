<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns="http://www.oracle.com/ns/ocep/jdbc"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:beans="http://www.springframework.org/schema/beans"
	targetNamespace="http://www.oracle.com/ns/ocep/jdbc"
	elementFormDefault="qualified" attributeFormDefault="unqualified">
	
        <xsd:import namespace="http://www.springframework.org/schema/beans" />

	<xsd:element name="jdbc-context">
            <xsd:annotation>
	        <xsd:documentation>
	            Custom tag to declare OCEP JDBC Cartridge context
                </xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
	        <xsd:complexContent>
		    <xsd:extension base="beans:identifiedType">
		    </xsd:extension>
	        </xsd:complexContent>
	    </xsd:complexType>
	</xsd:element>
</xsd:schema>
